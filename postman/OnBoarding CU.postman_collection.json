				],
				"body": {
					"mode": "raw",
					"raw": "{\r\n\t\"id\": \"{{reg_external_id}}\",\r\n    \"applicationId\": \"{{applicationId}}\",\r\n    \"certificationVersionId\": \"{{certificationVersionId}}\",\r\n    \"gatewayId\": \"{{reg_gateway_type}}\",\r\n    \"certificateType\": \"PEM\"\r\n}"
				},
